    "antd": "^3.13.3",
    "global": "^4.3.2",
    "highlight.js": "^9.14.2",
    "jquery": "^3.3.1",
    "marked": "^0.6.0",
    "react": "^16.8.1",
    "react-ace": "^6.4.0",
